{
    "mcpServers": {
        "workspace-mcp": {
            "command": "uvx",
            "args": [
                "workspace-mcp"
            ],
            "envFile": "${workspaceFolder}/.env",
            "type": "stdio"
        },
        "datadog-mcp": {
            "type": "stdio",
            "command": "$HOME/.local/bin/datadog_mcp_cli",
            "args": [
                "--region",
                "us"
            ],
            "env": {}
        },
        "atlassian-mcp": {
            "command": "npx",
                "args": [
                    "mcp-remote",
                    "https://mcp.atlassian.com/v1/sse"
                ]
        }
    }
}