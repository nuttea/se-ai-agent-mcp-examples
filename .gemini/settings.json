{
    "mcpServers": {
        "datadog-mcp": {
            "type": "http",
            "url": "https://mcp.datadoghq.com/api/unstable/mcp-server/mcp"
        },
        "workspace-mcp": {
            "command": "uvx",
            "args": [
                "workspace-mcp"
            ],
            "envFile": "${workspaceFolder}/.env",
            "type": "stdio"
        },
        "atlassian-mcp":{ 
            "url": "https://mcp.atlassian.com/v1/sse",
            "type": "http"
        }
    }
}