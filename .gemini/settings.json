{
    "mcpServers": {
        "workspace-mcp": {
            "command": "uvx",
            "args": [
                "workspace-mcp"
            ],
            "envFile": "${workspaceFolder}/.env",
            "type": "stdio"
        },
        "datadog-mcp": {
            "type": "stdio",
            "command": "$HOME/.local/bin/datadog_mcp_cli",
            "args": [
                "--region",
                "us"
            ],
            "env": {}
        },
        "atlassian-mcp":{ 
            "url": "https://mcp.atlassian.com/v1/sse",
            "type": "http"
        }
    }
}